hr.atk-hr-reset
section.atk-wrapper
  .atk-section-xlarge
    .atk-row
      {Action1}
      .atk-col-6
          .atk-cells
              a.atk-cell.atk-swatch-lightGray.atk-shape-rounded.further-action.atk-text-baseline-reset.atk-align-center(href="{$url_full}")
                span(class="icon-{$icon}").atk-size-pow9.atk-inline.atk-push-small
                .atk-size-mega.atk-text-medium {$name}
      {/}
      {Action2}
      .atk-col-6
          .atk-cells
              a.atk-cell.atk-swatch-lightGray.atk-shape-rounded.further-action.atk-text-baseline-reset.atk-align-center(href="{$url_full}")
                span(class="icon-{$icon}").atk-size-pow9.atk-inline.atk-push-small
                // span.icon-user-network-fix
                  span.icon-user-network.atk-size-pow10.atk-inline.atk-push-small.main
                  span.icon-user-network.atk-size-pow9.atk-inline.atk-push-small.hidden
                .atk-size-mega.atk-text-medium {$name}
      {/}
